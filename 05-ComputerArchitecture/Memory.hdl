CHIP Memory {
    IN in[16], load, address[15];
    OUT out[16];

    PARTS:
    DMux(in=load, sel=address[14], a=dmux1, b=dmux2);
    RAM16K(in=in, load=dmux1, address=address[0..13], out=ramOut);
    Screen(in=in, load=dmux2, address=address[0..12], out=screenOut);
    Keyboard(out=keyOut);
    And(a=address[13], b=address[14], out=andOut);
    Mux16(a=ramOut, b=screenOut, sel=address[14], out=muxOut);
    Mux16(a=muxOut, b=keyOut, sel=andOut, out=out);
}
